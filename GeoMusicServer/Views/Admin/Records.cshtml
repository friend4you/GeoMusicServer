@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";

}
<div class="container">
    <div class="row">
        <div class="col-md-9 col-sm-8">
            <h1 class="page-header">
                Admin Panel
            </h1>
            <table>
                <tr>
                    <th>
                        Название
                    </th>
                    <th>
                        Автор
                    </th>
                    <th>
                        Описание
                    </th>
                    <th>
                        Дата добавления
                    </th>
                    <th>
                        Изображение
                    </th>
                    <th>
                        URL
                    </th>
                    <th>
                        Latitude
                    </th>
                    <th>
                        Longitude
                    </th>
                    <th>

                    </th>

                </tr>

                @foreach (var record in ViewBag.Records)
                {
                    <tr>
                        <td>
                            @record.Title    |
                        </td>
                        <td>
                            @record.Artist    |
                        </td>
                        <td>
                            @record.Descroption    |
                        </td>
                        <td>
                            @record.AddDate |
                        </td>
                        <td>
                            @record.Image    |
                        </td>
                        <td>
                            @record.Url       |
                        </td>
                        <td>
                            @record.Lat    |
                        </td>
                        <td>
                            @record.Long    |
                        </td>
                        <td>
                            @Html.ActionLink("Редактировании", "Edit", new { id = record.Id }) |
                            @Html.ActionLink("Удалить", "Delete", new { id = record.Id })
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
    <!-- /.row -->
</div>